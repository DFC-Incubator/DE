<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.6.1//EN"
        "http://gwtproject.org/doctype/2.6.1/gwt-module.dtd">
<module>

    <!-- Highlight Appearance -->
    <replace-with class="org.iplantc.de.theme.base.client.applications.AppSearchHighlightAppearance">
        <when-type-is class="org.iplantc.de.theme.base.client.applications.AppSearchHighlightDefaultAppearance"/>
    </replace-with>

    <!-- Views -->
    <replace-with class="org.iplantc.de.theme.base.client.applications.AppsViewDefaultAppearance">
        <when-type-is class="org.iplantc.de.apps.client.AppsView.AppsViewAppearance"/>
    </replace-with>

    <!-- Cells -->
    <replace-with class="org.iplantc.de.theme.base.client.applications.cells.AppCommentCellDefaultAppearance">
        <when-type-is class="org.iplantc.de.apps.client.views.cells.AppCommentCell.AppCommentCellAppearance"/>
    </replace-with>

    <replace-with class="org.iplantc.de.theme.base.client.applications.cells.AppRatingCellDefaultAppearance">
        <when-type-is class="org.iplantc.de.apps.client.views.cells.AppRatingCell.AppRatingCellAppearance"/>
    </replace-with>

    <replace-with class="org.iplantc.de.theme.base.client.applications.cells.AppFavoriteCellDefaultAppearance">
        <when-type-is class="org.iplantc.de.apps.client.views.cells.AppFavoriteCell.AppFavoriteCellAppearance"/>
    </replace-with>

    <replace-with class="org.iplantc.de.theme.base.client.applications.cells.AppHyperlinkCellDefaultAppearance">
        <when-type-is class="org.iplantc.de.apps.client.views.cells.AppHyperlinkCell.AppHyperlinkCellAppearance"/>
    </replace-with>

    <replace-with class="org.iplantc.de.theme.base.client.applications.cells.AppInfoCellDefaultAppearance">
        <when-type-is class="org.iplantc.de.apps.client.views.cells.AppInfoCell.AppInfoCellAppearance"/>
    </replace-with>

    <!-- Submit -->
    <replace-with class="org.iplantc.de.theme.base.client.applications.submit.SubmitAppViewDefaultAppearance">
        <when-type-is class="org.iplantc.de.apps.client.SubmitAppForPublicUseView.SubmitAppAppearance"/>
    </replace-with>

    <!-- Toolbar -->
    <replace-with class="org.iplantc.de.theme.base.client.applications.toolbar.AppsToolbarViewDefaultAppearance">
        <when-type-is class="org.iplantc.de.apps.client.AppsToolbarView.AppsToolbarAppearance"/>
    </replace-with>
</module>