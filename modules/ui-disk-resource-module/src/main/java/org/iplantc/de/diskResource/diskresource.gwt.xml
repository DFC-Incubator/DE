<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.6.1//EN"
  "http://gwtproject.org/doctype/2.6.1/gwt-module.dtd">
<module>
    <!-- iPlant module inherits -->
    <inherits name="org.iplantc.de.commons.ui-commons"/>
    <inherits name="org.iplantc.de.collaborators.Collaborators" />
    <inherits name="org.iplantc.de.tags.Tags" />
        <!-- Deferred binding rules -->
    <replace-with class="org.iplantc.de.diskResource.client.views.toolbar.DiskResourceViewToolbarImpl">
       <when-type-is class="org.iplantc.de.diskResource.client.DiskResourceView.DiskResourceViewToolbar"/>
    </replace-with>
     <!-- Deferred binding rules -->
    <replace-with class="org.iplantc.de.diskResource.client.views.search.cells.DiskResourceSearchCellDefaultAppearance">
        <when-type-is class="org.iplantc.de.diskResource.client.views.search.cells.DiskResourceSearchCell.DiskResourceSearchCellAppearance"/>
    </replace-with>

    <source path="client"/>
</module>